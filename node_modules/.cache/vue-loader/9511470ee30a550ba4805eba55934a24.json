>\r\n    </el-upload>\r\n\r\n    <div class=\"report-list\" v-if=\"!showFileList\">\r\n      <p class=\"title\">报告结果</p>\r\n      <el-table :data=\"tableData\" stripe style=\"width: 90%\" border>\r\n        <el-table-column prop=\"index\" label=\"序列\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"name\" label=\"名称\" align=\"center\"></el-table-column>\r\n        <el-table-column label=\"预览\" width=\"400\" align=\"center\">\r\n          <template slot-scope=\"scope\">\r\n            <img :src=\"scope.row.url\" style=\"height: 90px\">\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"新冠肺炎结果\" align=\"center\">\r\n          <template slot-scope=\"scope\">\r\n            <span v-if=\"scope.row.isCovid19\" style=\"color:red\">阳性</span>\r\n            <span v-else>阴性</span>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <p style=\"color:#888; marginTop:30px\">报告结果-结束</p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      fileList: [],\r\n      localFileList: [],\r\n      showFileList: true,\r\n    };\r\n  },\r\n  computed: {\r\n    tableData() {\r\n      return this.localFileList.map((file, index) => ({\r\n        index: index + 1,\r\n        name: file.name.split(\".\")[0],\r\n        url: file.url,\r\n        isCovid19: Math.random() <= 0.3,\r\n      }));\r\n    },\r\n  },\r\n  methods: {\r\n    handlePreview(file, fileList) {\r\n      console.log(fileList);\r\n      window.open(file?.url, \"_blank\");\r\n    },\r\n    handleRemove(file) {\r\n      this.localFileList = this.localFileList.filter(\r\n        (fL) => file.url !== fL.url\r\n      );\r\n    },\r\n    handleUpdate(file) {\r\n      this.showFileList = true\r\n      this.localFileList.push(file);\r\n    },\r\n    doPredict(){\r\n        this.showFileList = false\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.predict {\r\n  margin: 0 auto;\r\n  width: 100%;\r\n//   border: 1px solid red;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  .title {\r\n    font-size: 25px;\r\n    font-weight: 600;\r\n  }\r\n  .en-title{\r\n      font-size: 20px;\r\n      font-weight: 200;\r\n      margin-bottom: 60px;\r\n  }\r\n}\r\n.report-list {\r\n  width: 70%;\r\n  border: 3px solid black;\r\n  background-color: #fff;\r\n  border-radius: 10px;\r\n  margin: 40px 0;\r\n//   min-height: 600px;\r\n  padding: 10px 50px 10px 50px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n}\r\n</style>\r\n\r\n<style lang=\"less\">\r\n.el-upload-list {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  margin: 20px 0;\r\n  width: 70vw;\r\n//   border: 1px solid green;\r\n  min-height: 200px;\r\n\r\n  .el-upload-list__item {\r\n    margin: 10px 20px!important;\r\n    width: 300px !important;\r\n  }\r\n}\r\n\r\n</style>"]}]}