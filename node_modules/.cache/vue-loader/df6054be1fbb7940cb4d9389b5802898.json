ataset2, dataset3, dataset4])[this.dataset - 1]\n      return resultData.filter(post => {\n        for(let para of post){\n          const { type, content } = para\n          if(type === 'text' && JSON.stringify(content).indexOf(this.query) > 0){\n            return true\n          }\n        }\n        return false\n      });\n    }\n  },\n  components:{\n    Post\n  }\n};\n</script>\n\n\n<style lang=\"less\" scoped>\n.title {\n    font-size: 30px;\n    color: #fff;\n    font-weight: 600;\n  }\n  .en-title {\n    color: #999;\n    font-size: 25px;\n    font-weight: 200;\n    margin-bottom: 20px;\n  }\n.detail-results {\n  width: 80%;\n  margin: 0 auto;\n  //   padding: 50px 0;\n}\n.post-outer-container{\n  height: 600px;\n  overflow-y: auto;\n}\n\n</style>\n\n<style lang=\"less\">\n.el-table .cell {\n  line-height: 34px;\n}\n</style>\n"]}]}